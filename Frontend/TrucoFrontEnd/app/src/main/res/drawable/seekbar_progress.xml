<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#D3D3D3"/> <!-- You can modify this color for the background of the SeekBar -->
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="#0000FF"/> <!-- Blue color for the progress -->
            </shape>
        </clip>
    </item>
</layer-list>
